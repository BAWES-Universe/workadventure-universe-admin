{
  "name": "workadventure-universe-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest --testPathIgnorePatterns=__tests__/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "./scripts/test-api.sh",
    "test:integration": "jest --testPathPatterns=__tests__/integration",
    "test:integration:watch": "jest --testPathPatterns=__tests__/integration --watch",
    "test:full": "./scripts/run-full-test.sh",
    "setup": "./scripts/setup-and-test.sh",
    "fix:prisma": "./scripts/fix-prisma.sh",
    "db:fix": "./scripts/fix-prisma-complete.sh",
    "db:fix:quick": "docker exec admin-api-dev sh -c 'cd /app && npx prisma migrate deploy && npx prisma generate'",
    "postinstall": "prisma generate",
    "db:generate-url": "bash -c 'eval $(scripts/generate-database-url.sh) && echo $DATABASE_URL'",
    "db:migrate": "docker exec admin-api-dev sh -c 'cd /app && npx prisma migrate dev'",
    "db:migrate:deploy": "docker exec admin-api-dev sh -c 'cd /app && npx prisma migrate deploy'",
    "db:studio": "docker exec -it admin-api-dev sh -c 'cd /app && HOST=0.0.0.0 npx prisma studio --port 5555 --browser none'",
    "db:generate": "docker exec admin-api-dev sh -c 'cd /app && npx prisma generate'",
    "db:push": "docker exec admin-api-dev sh -c 'cd /app && npx prisma db push'",
    "db:reset": "docker exec admin-api-dev sh -c 'cd /app && npx prisma migrate reset --force'",
    "db:seed": "docker exec admin-api-dev sh -c 'cd /app && npx prisma db seed'"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.8.2",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.561.0",
    "next": "^16.0.7",
    "openid-client": "^6.8.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "redis": "^5.10.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@workadventure/iframe-api-typings": "^1.27.9",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
